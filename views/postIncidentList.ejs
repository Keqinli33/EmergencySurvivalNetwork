<div class="container" ng-controller="postIncidentListCtrl" ng-show="showList.postIncidentList">
    <div class="container">
        <h3> Update My Incidents </h3>
<table class="table">
    <thead>
    <tr>
        <th>
            contents
        </th>
        <th>
            time
        </th>
        <th>Update</th>
    </tr>
    </thead>
    <tr ng-repeat="content in contents">
        <td>
             {{content.content}}
        </td>
        <td>
            {{ content.postTime | date: 'MM.dd.yyyy HH:mma'}}
        </td>
        <td><button ng-click="openUpdate(content.postTime)">Update</button></td>
    </tr>
</table>
    </div>
</div>